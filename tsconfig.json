{
	"extends": "astro/tsconfigs/strict",
	"include": [".astro/types.d.ts", "**/*"],
	"exclude": ["dist"],
	"compilerOptions": {
		"baseUrl": "./",
		"paths": {
			"@layouts/*": ["src/layouts/*"],
			"@pages/*": ["src/pages/*"],
			"@data/*": ["src/lib/data/*"],
			"@static/*": ["src/lib/static/*"],
			"@components/*": ["src/components/*"],
			"@utils/*": ["src/lib/utils/*"],
			"@fetch/*": ["src/lib/fetch/*"],
			"@public/*": ["public/*"]
		}
	}
}
